  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev:spotlight": "npx @spotlightjs/spotlight",
    "dev:next": "next dev --turbopack",
    "dev": "run-p dev:next codegen:watch",
    "build:next": "next build",
    "build": "node scripts/build.js",
    "build:ci": "npm run build:next",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:types": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "commit": "commit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "codegen": "node scripts/codegen-with-retry.js",
    "codegen:watch": "node scripts/codegen-with-retry.js --watch"
  },
